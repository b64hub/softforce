<?xml version="1.0" encoding="UTF-8" ?>
<AuthProvider xmlns="http://soap.sforce.com/2006/04/metadata">
    <customMetadataTypeRecord
  >Softrig_Auth_Settings__mdt.Softrig</customMetadataTypeRecord>
    <executionUser>{env.EXEC_USER}</executionUser>
    <friendlyName>Softrig</friendlyName>
    <iconUrl
  >https://www.softrig.com/media/site/softrig-symbol-blue.svg</iconUrl>
    <includeOrgIdInIdentifier>false</includeOrgIdInIdentifier>
    <plugin>SoftrigAuthProvider</plugin>
    <providerType>Custom</providerType>
    <sendAccessTokenInHeader>false</sendAccessTokenInHeader>
    <sendClientCredentialsInHeader>false</sendClientCredentialsInHeader>
    <sendSecretInApis>true</sendSecretInApis>
</AuthProvider>
